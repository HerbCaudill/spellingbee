{
  "name": "spellingbee",
  "description": "Clone of NYT Spelling Bee game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "fast-memoize": "^2.5.2",
    "postcss-cli": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "tailwindcss": "^1.8.9",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:words": "ts-node --project ./src/scripts/tsconfig.json ./src/scripts/buildWordList.ts",
    "build:css": "postcss src/styles.postcss -o public/styles.css",
    "build:react": "react-scripts build",
    "build": "run-s build:*",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
